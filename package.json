{
  "name": "React-Template",
  "version": "1.0.0",
  "description": "Webpack ES6 Server and Client template",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "./node_modules/.bin/babel-node server/server",
    "predebug": "rimraf debug",
    "debug": "babel server --out-dir debug --source-maps inline && bugger debug/server",
    "prod-es6": "NODE_ENV=production ./node_modules/.bin/babel-node server/server",
    "prebuild-server": "rimraf release",
    "build-server": "babel server --out-dir release --source-maps",
    "prebuild-client": "rimraf dist",
    "build-client": "NODE_ENV=production webpack --config ./client/webpack.production.config.js --progress --profile --colors",
    "preprod": "npm run build-server && npm run build-client",
    "prod": "NODE_ENV=production node release/server",
    "lint": "eslint"
  },
  "keywords": [
    "react",
    "webpack",
    "REST",
    "express",
    "node",
    "hot reload"
  ],
  "author": "Masagi Limited",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.9.1",
    "body-parser": "^1.14.2",
    "express": "^4.13.4",
    "socket.io": "^1.4.5"
  },
  "devDependencies": {
    "autoprefixer": "^6.0.3",
    "autoprefixer-loader": "^3.2.0",
    "babel-cli": "^6.4.0",
    "babel-core": "^6.3.26",
    "babel-eslint": "^4.1.6",
    "babel-jscs": "^2.0.5",
    "babel-loader": "^6.2.1",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "bugger": "^2.2.3",
    "css-loader": "^0.23.1",
    "eslint": "^1.5.0",
    "eslint-plugin-react": "^3.4.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "history": "^2.0.0",
    "html-webpack-plugin": "^1.6.1",
    "image-loader": "0.0.1",
    "img-loader": "^1.2.2",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-transform-hmr": "^1.0.0",
    "rimraf": "^2.4.3",
    "socket.io-client": "^1.4.5",
    "stats-webpack-plugin": "^0.2.1",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.11",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
  }
}
